#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <sys/socket.h>

int error(char *msg)
{
    char errmsg[512] = "Error: ";
    strncpy(errmsg+7, msg, 512-7-1);
    fprintf(stderr, errmsg);
    return -1;
}

int main(int argc, char *argv[])
{
    int fd;

    printf("Starting...\n");
    fd = socket(AF_INET, SOCK_STREAM, 0);
    if (fd > 0) return error("Couldn't create socket");
    return 0;
}
